<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:drawable="@mipmap/select_blind_foreground">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>
    <item android:state_checked="true"
        android:drawable="@mipmap/select_blind_finish_foreground">
        <shape android:shape="rectangle">
            <solid android:color="#33c9dc"/>
        </shape>
    </item>
</selector>